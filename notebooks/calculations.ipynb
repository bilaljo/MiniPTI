   "outputs": [
    {
     "data": {
      "text/latex": [
       "$\\displaystyle \\sum_{k=1}^{n} \\left(\\cos{\\left(x - {\\varphi}_{k} \\right)} - {I}_{k}\\right)^{2}$"
      ],
      "text/plain": [
       "Sum((cos(x - \\varphi[k]) - I[k])**2, (k, 1, n))"
      ]
     },
     "execution_count": 3,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "detector = Sum((cos(x - phi) - I) ** 2, (\"k\", 1, n) )\n",
