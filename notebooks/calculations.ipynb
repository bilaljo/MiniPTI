   "outputs": [],
   "source": [
    "x, n = symbols(\"x, n\")\n",
    "I, phi = Indexed(\"I\", \"k\"), Indexed(r\"\\varphi\", \"k\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
